<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.8.2 -->
<interface>
  <!-- interface-name MainWindow.ui -->
  <requires lib="gtk" version="4.0"/>
  <template class="MainWindow" parent="GtkApplicationWindow">
    <property name="default-height">384</property>
    <property name="default-width">683</property>
    <property name="title">Python e GTK 4: Criando executáveis com Cx_Freeze.</property>
    <child>
      <object class="GtkBox">
        <property name="margin-bottom">12</property>
        <property name="margin-end">12</property>
        <property name="margin-start">12</property>
        <property name="margin-top">12</property>
        <property name="orientation">vertical</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkEntry" id="entry">
            <property name="placeholder-text">Digite algo</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label">
            <property name="label">Digite algo no campo de texto e clique no botão.</property>
            <property name="vexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkButton">
            <property name="label">Clique aqui</property>
            <signal name="clicked" handler="on_button_clicked" swapped="no"/>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
