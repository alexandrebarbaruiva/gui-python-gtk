<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk+" version="3.20"/>
    <template class="MainWindow" parent="GtkApplicationWindow">
        <property name="can_focus">False</property>
        <property name="default_width">683</property>
        <property name="default_height">384</property>
        <property name="title" translatable="yes">Dark Mode</property>
        <property name="icon">../../../../../images/icons/icon.png</property>
        <child type="titlebar">
            <object class="GtkHeaderBar" id="HeaderBar">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="title" translatable="yes">Dialogo</property>
                <property name="subtitle" translatable="yes">Salvar arquivo</property>
                <property name="show_close_button">True</property>
            </object>
        </child>
        <child>
            <object class="GtkBox">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="border_width">12</property>
                <property name="orientation">vertical</property>
                <child>
                    <object class="GtkButton" id="btn_open_dialog">
                        <property name="label" translatable="yes">Abrir di√°logo</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <signal name="clicked" handler="_open_dialog" swapped="no"/>
                    </object>
                    <packing>
                        <property name="expand">True</property>
                        <property name="fill">False</property>
                        <property name="position">0</property>
                    </packing>
                </child>
            </object>
        </child>
    </template>
</interface>