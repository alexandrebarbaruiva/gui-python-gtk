<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk+" version="3.20"/>
    <template class="MainWindow" parent="GtkApplicationWindow">
        <property name="can_focus">False</property>
        <property name="title" translatable="yes">Fixed Layout</property>
        <property name="default_width">683</property>
        <property name="default_height">384</property>
        <property name="icon">../../../../../images/icons/icon.png</property>
        <child>
            <placeholder/>
        </child>
        <child>
            <object class="GtkFixed" id="fixed">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="border_width">12</property>
                <child>
                    <object class="GtkButton">
                        <property name="label" translatable="yes">Clique Aqui</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <signal name="clicked" handler="_on_button_clicked" swapped="no"/>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
