<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk+" version="3.20"/>
    <template class="MainWindow" parent="GtkApplicationWindow">
        <property name="can_focus">False</property>
        <property name="title" translatable="yes">FlowBox Layout</property>
        <property name="default_width">683</property>
        <property name="default_height">384</property>
        <property name="icon">../../../../assets/icons/icon.png</property>
        <child>
            <placeholder/>
        </child>
        <child>
            <object class="GtkScrolledWindow">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="shadow_type">in</property>
                <child>
                    <object class="GtkViewport">
                        <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <child>
                            <object class="GtkFlowBox" id="flowbox">
                                <property name="visible">True</property>
                                <property name="can_focus">False</property>
                                <property name="border_width">12</property>
                                <property name="max_children_per_line">5</property>
                                <property name="selection_mode">none</property>
                                <property name="activate_on_single_click">False</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
